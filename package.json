    "express": "^4.13.4",
    "halogenium": "^2.3.0",
    "history": "^1.17.0",
    "js-cookie": "^2.2.1",
    "morgan": "^1.6.1",
    "node-vibrant": "^3.1.5",
    "object-assign": "^4.0.1",
    "querystring": "^0.2.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-helmet": "^6.1.0",
    "react-hot-loader": "^1.3.0",
    "react-redux": "^7.2.1",
    "react-router": "^5.2.0",
    "react-router-redux": "^3.0.0",
    "react-transform-hmr": "^1.0.2",
    "redbox-react": "^1.2.2",
    "redux": "^3.2.1",
