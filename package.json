{
  "name": "spotify-visual-app",
  "version": "0.0.1",
  "description": "Analyze the tone of your Spotify library with IBM Watson",
  "main": "client/index.js",
  "homepage": "https://spotify-visual-app.herokuapp.com",
  "scripts": {
    "start": "better-npm-run start",
    "predeploy": "better-npm-run build",
    "deploy": "gh-pages -d dist",
    "build": "better-npm-run build",
    "dev": "better-npm-run dev",
    "lint": "eslint --ignore-path .gitignore ."
  },
  "betterScripts": {
    "build": {
      "command": "webpack --config webpack/prod.config.js --progress --colors",
      "env": {
        "NODE_ENV": "production"
      }
    },
    "start": {
      "command": "node server/app.js",
      "env": {
        "NODE_ENV": "production"
      }
    },
    "dev": {
      "command": "node server/app.js",
      "env": {
        "NODE_ENV": "development"
      }
    }
  },
  "dependencies": {
    "@material-ui/core": "^4.11.0",
